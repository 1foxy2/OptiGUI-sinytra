{
  "schemaVersion": 1,
  "id": "optigui",
  "version": "${version}",
  "name": "OptiGUI",
  "description": "Custom inventory GUIs on Fabric&Quilt with OptiFine resource pack support&many more features",
  "provides": [
    "optigui-api"
  ],
  "authors": [
    "opekope2"
  ],
  "contact": {
    "homepage": "https://opekope2.github.io/OptiGUI",
    "sources": "https://github.com/opekope2/OptiGUI",
    "issues": "https://github.com/opekope2/OptiGUI/issues"
  },
  "license": "MIT",
  "icon": "assets/optigui/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.impl.OptiGuiApi"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.InspectorKeyBinding"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.TextureReplacer"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.interaction.InteractionHandler"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.interaction.Processors::loadEntityProcessors"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.interaction.Processors::loadBlockEntityProcessors"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.Selectors::loadSelectors"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.Selectors::loadLoadTimeSelectors"
      }
    ],
    "optigui-entityprocessor": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.GenericEntityProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.ComparableEntityProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.ChestBoatProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.HorseProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.DonkeyProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.LlamaProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.VillagerProcessor"
      }
    ],
    "optigui-blockentityprocessor": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.GenericBlockEntityProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.ComparableBlockEntityProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.BeaconProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.ChestProcessor"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.LecternProcessor"
      }
    ],
    "optigui-selector": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.BeaconLevelSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.BookPageSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.BookPageCountSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.ChestBoatVariantSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.LargeChestSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.NameSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.WildcardNameSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.CaseInsensitiveWildcardNameSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.RegexNameSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.CaseInsensitiveRegexNameSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.BiomeSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.HeightSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.DonkeyChestSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.HorseVariantSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.HorseMarkingSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.LlamaColorSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.LlamaVariantSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.DateSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.InteractionTextureSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.InteractionHandSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.RedstoneComparatorOutputSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.VillagerProfessionSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.VillagerTypeSelector"
      }
    ],
    "optigui-load-selector": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.ConditionalLoadSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.ModLoadSelector"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.selector.LegacyOptiGuiVersionLoadSelector"
      }
    ],
    "lilac-resourceloader-factory": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.lilac_resource_loader.OptiGuiResourceLoader\u0024Factory"
      },
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.lilac_resource_loader.OptiFineResourceLoader\u0024Factory"
      }
    ],
    "lilac-resourceloadingsession-extensionfactory": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.lilac_resource_loader.OptiGuiResourceLoadingSession\u0024ExtensionFactory"
      }
    ],
    "lilac-resourceloadingsession-lifecyclelistener": [
      {
        "adapter": "kotlin",
        "value": "opekope2.optigui.internal.lilac_resource_loader.OptiGuiResourceLoadingSession\u0024ExtensionFactory"
      }
    ]
  },
  "mixins": [
    "optigui.mixins.json"
  ],
  "accessWidener": "optigui.accesswidener",
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-networking-api-v1": "*",
    "fabric-events-interaction-v0": "*",
    "fabric-key-binding-api-v1": "*",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=1.18 <=1.20.1",
    "java": ">=${java}",
    "lilac": ">=1.0.0-alpha.1 <1.1.0"
  },
  "recommends": {
    "animatica": "*"
  },
  "custom": {
    "mc-publish": {
      "loaders": [
        "fabric",
        "quilt"
      ],
      "dependencies": [
        "fabric-api(required)"
